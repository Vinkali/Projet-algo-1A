public class Personnage{
    private String nom;
    private int pV;
    private Attaque attaque1= new Attaque();
    private Attaque attaque2= new Attaque();
    
    public Personnage(String nom, int vie, int atk1, int atk2){
        this.nom = nom;
        this.pV = vie;
        this.attaque1 = atk2;
        this.attaque2 = atk2;
    }
    
    public Personnage(int i){
        if(i==1){
            this.nom = "Hector";
            this.pV = 10;
            this.attaque1 = Attaque("Tir arc", 6, 20);
            this.attaque2 = Attaque("Coup de poing", 1, 50);
        }else if(){
        
        }
    }
    
    public String getNom(){
        return this.nom;
    }
    
    public String getPV(){
        return this.pV;
    }
    
    public void degat(int degat){
        this.pV = this.pV - degat;
    }
    
}

